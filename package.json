{
  "name": "minimal-snippets",
  "displayName": "Minimal Snippets",
  "description": "unos pocos pero utiles snippets para css html react tailwind y express",
  "version": "0.0.9",
  "publisher": "Paku",
  "repository": "https://github.com/pa-ku/Minimal-Snippets",
  "icon": "icon.webp",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
